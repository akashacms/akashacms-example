{
  "author": {
    "name": "David Herron",
    "email": "david@davidherron.com",
    "url": "http://davidherron.com"
  },
  "name": "akashacms-example",
  "description": "AkashaCMS Example website",
  "version": "0.6.5",
  "repository": {
    "type": "git",
    "url": "git://github.com/akashacms/akashacms-example.git"
  },
  "scripts": {
    "prebuild": "npm run build-vue-js && akasharender copy-assets config.js",
    "build": "akasharender render config.js",
    "build-vue-js": "npm run build-vue-js-example-1",
    "build-vue-js-example-1": "cd vue-js-examples/example-1 && npm install && npm run build",
    "deploy": "cd out && rsync --archive --delete --verbose ./ user-name@example.akashacms.com:example.akashacms.com/ ",
    "preview": "cd out && ../node_modules/.bin/hostr"
  },
  "dependencies": {
    "akashacms-affiliates": "github:akashacms/akashacms-affiliates",
    "akashacms-base": "github:akashacms/akashacms-base",
    "akashacms-booknav": "github:akashacms/akashacms-booknav",
    "akashacms-breadcrumbs": "github:akashacms/akashacms-breadcrumbs",
    "akashacms-dlassets": "github:akashacms/akashacms-dlassets",
    "akashacms-document-viewers": "github:akashacms/akashacms-document-viewers",
    "akashacms-embeddables": "github:akashacms/akashacms-embeddables",
    "akashacms-external-links": "github:akashacms/akashacms-external-links",
    "akashacms-footnotes": "github:akashacms/akashacms-footnotes",
    "akashacms-tagged-content": "github:akashacms/akashacms-tagged-content",
    "akashacms-theme-bootstrap": "github:akashacms/akashacms-theme-bootstrap",
    "akasharender": "github:akashacms/akasharender",
    "bootstrap": "^3.3.7",
    "cheerio": "*",
    "debug": "^2.6.9",
    "epub-skeleton": "github:akashacms/epub-skeleton",
    "epub-website": "github:akashacms/epub-website",
    "fs-extra": "^5.x",
    "globfs": ">=0.3",
    "hostr": "^2.x",
    "jquery": "^3.1.1",
    "mahabhuta": "github:akashacms/mahabhuta",
    "oembetter": "^0.1.19"
  }
}
