	<!-- Our site title and description -->
	<% if (typeof pagetitle !== "undefined") { %>
		<title><%= pagetitle %></title>
	<% } else if (typeof title !== "undefined") { %>
		<title><%= title %></title>
	<% } %>
		
<%
    if (typeof metaOGtitle === "undefined") {
	if (typeof pagetitle !== "undefined") {
		metaOGtitle = pagetitle;
	} else if (typeof title !== "undefined") {
		metaOGtitle = title;
	}
    }
    if (typeof metaOGdescription === "undefined") {
	if (typeof metadescription !== "undefined") {
		metaOGdescription = metadescription;
	}
    }
    if (typeof metaDCtitle === "undefined") {
	if (typeof pagetitle !== "undefined") {
		metaDCtitle = pagetitle;
	} else if (typeof title !== "undefined") {
		metaDCtitle = title;
	}
    }
    if (typeof metapagename === "undefined") {
	if (typeof pagetitle !== "undefined") {
		metapagename = pagetitle;
	} else if (typeof title !== "undefined") {
		metapagename = title;
	}
    }
    if (typeof metadate === "undefined") {
	metadate = rendered_date;
    }

%>

	
